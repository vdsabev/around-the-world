{
  "name": "around-the-world",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "netlify dev",
    "build": "npm run build.favicon && npm run build.logo && react-scripts build",
    "build.favicon": "icon-gen -i src/logo.svg -o public -r --favicon name=favicon ico=16,24,32,64 sizes=",
    "build.logo": "icon-gen -i src/logo.svg -o public -r --favicon name=logo ico= sizes=192,512",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "0.11.0",
    "@slack/web-api": "6.0.0",
    "googleapis": "67.0.0",
    "icon-gen": "2.1.0",
    "lessttp": "0.2.4",
    "lodash": "4.17.20",
    "mapbox-gl": "2.0.1",
    "mongodb": "3.6.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    }
  }
}
